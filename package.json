{
  "name": "s3db.js",
  "version": "4.0.1",
  "description": "Use AWS S3, the world's most reliable document storage, as a database with this ORM.",
  "main": "dist/s3db.cjs.js",
  "module": "dist/s3db.es.js",
  "browser": "dist/s3db.iife.js",
  "types": "dist/s3db.d.ts",
  "unpkg": "dist/s3db.iife.min.js",
  "jsdelivr": "dist/s3db.iife.min.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/s3db.es.js",
      "require": "./dist/s3db.cjs.js",
      "browser": "./dist/s3db.iife.js",
      "default": "./dist/s3db.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "forattini-dev",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forattini-dev/s3db.js.git"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "keywords": [
    "s3",
    "aws",
    "database"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean-build": "node scripts/clean-build.js",
    "postbuild": "node scripts/prefix-files-istanbul-ignore.js",
    "verify": "node scripts/verify-build.js",
    "test-builds": "node examples/test-builds.js",
    "coverage": "coveralls < coverage/lcov.info",
    "coverage:serve": "npx http-server ./coverage/lcov-report",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --detectOpenHandles",
    "test:watch": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --watch --detectOpenHandles",
    "test:dist": "node --no-warnings --experimental-vm-modules ./tests/dist.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.837.0",
    "@supercharge/promise-pool": "^3.2.0",
    "avsc": "^5.7.7",
    "fastest-validator": "^1.19.1",
    "flat": "^6.0.1",
    "json-stable-stringify": "^1.3.0",
    "lodash-es": "^4.17.21",
    "nanoid": "5.1.5",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@jest/globals": "^30.0.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^30.0.2",
    "cliui": "^9.0.1",
    "coveralls": "^3.1.1",
    "dotenv": "^16.6.1",
    "esbuild": "^0.25.5",
    "fakerator": "^0.3.6",
    "jest": "^30.0.3",
    "multi-progress": "^4.0.0",
    "progress": "^2.0.3",
    "rollup": "^4.44.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0"
  }
}
