{
  "name": "s3db.js",
  "version": "19.3.3",
  "description": "Use AWS S3, the world's most reliable document storage, as a database with this ORM.",
  "main": "dist/s3db.cjs",
  "module": "dist/s3db.es.js",
  "types": "dist/s3db.d.ts",
  "author": "@stone/martech",
  "license": "UNLICENSED",
  "bin": {
    "s3db.js": "./bin/cli.js",
    "s3db": "./bin/cli.js",
    "s3db-mcp": "./mcp/entrypoint.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forattini-dev/s3db.js.git"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "keywords": [
    "s3",
    "aws",
    "database",
    "orm",
    "nosql",
    "document-store",
    "cloud-database",
    "metadata-encoding",
    "s3-database",
    "serverless",
    "mcp",
    "model-context-protocol",
    "mcp-server",
    "claude",
    "ai-tools",
    "llm"
  ],
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#mcp/*": "./mcp/*",
    "#dist/*": "./dist/*",
    "#examples/*": "./examples/*",
    "#src/*": "./src/*",
    "#tests/*": "./tests/*"
  },
  "exports": {
    ".": {
      "types": "./dist/s3db.d.ts",
      "import": "./dist/s3db.es.js",
      "require": "./dist/s3db.cjs"
    },
    "./lite": {
      "types": "./dist/s3db-lite.d.ts",
      "import": "./dist/s3db-lite.es.js",
      "require": "./dist/s3db-lite.cjs"
    },
    "./typescript-generator": {
      "types": "./src/concerns/typescript-generator.d.ts",
      "import": "./src/concerns/typescript-generator.js",
      "require": "./src/concerns/typescript-generator.js"
    },
    "./concerns/guards-helpers": {
      "import": "./src/concerns/guards-helpers.js",
      "require": "./src/concerns/guards-helpers.js"
    },
    "./src/*": "./src/*"
  },
  "files": [
    "dist/",
    "src/",
    "bin/cli.js",
    "mcp/",
    "README.md",
    "PLUGINS.md",
    "SECURITY.md",
    "UNLICENSE"
  ],
  "scripts": {
    "build": "npm run build:core",
    "build:core": "tsc -p tsconfig.build.json && rollup -c",
    "build:types": "rollup -c rollup.dts.config.js",
    "build:all": "npm run build:core && npm run build:types",
    "typecheck": "tsc --noEmit",
    "build:binaries": "./scripts/scripts/build-binaries.sh",
    "dev": "rollup -c -w",
    "test": "pnpm run test:core",
    "test:all": "pnpm run test:core && pnpm run test:plugins:fast && pnpm run test:plugins:slow && pnpm run test:plugins:api && pnpm run test:plugins:spider",
    "test:core": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/core/",
    "test:plugins": "pnpm run test:plugins:fast && pnpm run test:plugins:slow && pnpm run test:plugins:api && pnpm run test:plugins:spider",
    "test:plugins:fast": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/plugins/state-machine/ tests/plugins/plugin-storage/ tests/plugins/ttl/ tests/plugins/s3-queue/",
    "test:plugins:slow": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/plugins/scheduler/ tests/plugins/tfstate/ tests/plugins/replicator/ tests/plugins/replicators/ tests/plugins/concerns/",
    "test:plugins:api": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/plugins/api/",
    "test:plugins:spider": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/plugins/spider/",
    "test:plugins:root": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run --dir tests/plugins --ignore 'tests/plugins/*/'",
    "test:memory": "pnpm run test:memory:core && pnpm run test:memory:plugins && pnpm run test:ts",
    "test:memory:core": "S3DB_LOG_LEVEL=warn TEST_FORCE_MEMORY_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/core/ --no-file-parallelism",
    "test:memory:plugins": "S3DB_LOG_LEVEL=warn TEST_FORCE_MEMORY_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/plugins/ --no-file-parallelism",
    "test:s3": "pnpm run test:s3:core && pnpm run test:s3:plugins && pnpm run test:ts",
    "test:s3:core": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=false BUCKET_CONNECTION_STRING='http://minioadmin:minioadmin123@localhost:9100/s3db' node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/core/",
    "test:s3:plugins": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=false BUCKET_CONNECTION_STRING='http://minioadmin:minioadmin123@localhost:9100/s3db' node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run tests/plugins/",
    "test:ts": "tsc --noEmit --project tests/typescript/tsconfig.json",
    "test:coverage": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs run --coverage && pnpm run test:ts",
    "test:watch": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs",
    "test:ui": "S3DB_LOG_LEVEL=warn TEST_USE_FILESYSTEM_CLIENT=true node --max-old-space-size=4096 node_modules/vitest/vitest.mjs --ui",
    "benchmark": "node benchmark-compression.js",
    "benchmark:partitions": "node docs/benchmarks/partitions-matrix.js",
    "release:check": "./scripts/pre-release-check.sh",
    "validate:types": "pnpm run test:ts && echo 'TypeScript definitions are valid!'",
    "test:ts:runtime": "tsx tests/typescript/types-runtime-simple.ts",
    "test:mcp": "tsx mcp/entrypoint.ts --help",
    "install:peers": "pnpm add -D @aws-sdk/client-acm @aws-sdk/client-api-gateway @aws-sdk/client-apigatewayv2 @aws-sdk/client-backup @aws-sdk/client-cloudfront @aws-sdk/client-cloudtrail @aws-sdk/client-cloudwatch @aws-sdk/client-cloudwatch-logs @aws-sdk/client-cognito-identity-provider @aws-sdk/client-config-service @aws-sdk/client-dynamodb @aws-sdk/client-ec2 @aws-sdk/client-ecr @aws-sdk/client-ecs @aws-sdk/client-efs @aws-sdk/client-eks @aws-sdk/client-elasticache @aws-sdk/client-elastic-load-balancing @aws-sdk/client-elastic-load-balancing-v2 @aws-sdk/client-eventbridge @aws-sdk/client-iam @aws-sdk/client-kinesis @aws-sdk/client-kms @aws-sdk/client-lambda @aws-sdk/client-rds @aws-sdk/client-route-53 @aws-sdk/client-secrets-manager @aws-sdk/client-sfn @aws-sdk/client-sns @aws-sdk/client-sqs @aws-sdk/client-ssm @aws-sdk/client-sts @aws-sdk/client-waf @aws-sdk/client-wafv2 @google-cloud/bigquery google-auth-library @hono/node-server @hono/swagger-ui @libsql/client @planetscale/database @tensorflow/tfjs-node amqplib bcrypt express hono jose nodemailer pg",
    "install:peers:script": "./scripts/install-peer-deps.sh",
    "install:dev:core": "echo '✅ Core dependencies already installed via pnpm install'",
    "install:dev:replicators": "pnpm add -D pg @google-cloud/bigquery google-auth-library @planetscale/database @libsql/client @aws-sdk/client-sqs amqplib",
    "install:dev:plugins": "pnpm add -D hono @hono/node-server @hono/swagger-ui jose bcrypt nodemailer @tensorflow/tfjs-node",
    "install:dev:puppeteer": "pnpm add -D puppeteer-extra puppeteer-extra-plugin-stealth user-agents ghost-cursor",
    "install:dev:cloud": "pnpm add -D flat @aws-sdk/client-acm @aws-sdk/client-api-gateway @aws-sdk/client-apigatewayv2 @aws-sdk/client-backup @aws-sdk/client-cloudfront @aws-sdk/client-cloudtrail @aws-sdk/client-cloudwatch @aws-sdk/client-cloudwatch-logs @aws-sdk/client-cognito-identity-provider @aws-sdk/client-config-service @aws-sdk/client-dynamodb @aws-sdk/client-ec2 @aws-sdk/client-ecr @aws-sdk/client-ecs @aws-sdk/client-efs @aws-sdk/client-eks @aws-sdk/client-elasticache @aws-sdk/client-elastic-load-balancing @aws-sdk/client-elastic-load-balancing-v2 @aws-sdk/client-eventbridge @aws-sdk/client-iam @aws-sdk/client-kinesis @aws-sdk/client-kms @aws-sdk/client-lambda @aws-sdk/client-rds @aws-sdk/client-route-53 @aws-sdk/client-secrets-manager @aws-sdk/client-sfn @aws-sdk/client-sns @aws-sdk/client-sqs @aws-sdk/client-ssm @aws-sdk/client-sts @aws-sdk/client-waf @aws-sdk/client-wafv2",
    "install:dev:minimal": "pnpm run install:dev:core && echo '✅ Minimal dev setup complete (core only)'",
    "install:dev:common": "pnpm run install:dev:core && pnpm run install:dev:replicators && pnpm run install:dev:plugins && echo '✅ Common dev setup complete (core + replicators + plugins)'",
    "install:dev:full": "pnpm run install:dev:core && pnpm run install:dev:replicators && pnpm run install:dev:plugins && pnpm run install:dev:puppeteer && pnpm run install:dev:cloud && echo '✅ Full dev setup complete (all dependencies)'",
    "docs": "serve docs -p 3000",
    "build:embeddings": "tsx scripts/build-embeddings.ts",
    "mcp:reindex": "tsx scripts/build-embeddings.ts"
  },
