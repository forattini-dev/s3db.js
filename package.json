{
  "name": "s3db.js",
  "version": "11.3.1",
  "description": "Use AWS S3, the world's most reliable document storage, as a database with this ORM.",
  "main": "dist/s3db.cjs.js",
  "module": "dist/s3db.es.js",
  "types": "dist/s3db.d.ts",
  "author": "@stone/martech",
  "license": "UNLICENSED",
  "bin": {
    "s3db.js": "./bin/cli.js",
    "s3db": "./bin/cli.js",
    "s3db-mcp": "./mcp/entrypoint.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forattini-dev/s3db.js.git"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "keywords": [
    "s3",
    "aws",
    "database",
    "orm",
    "nosql",
    "document-store",
    "cloud-database",
    "metadata-encoding",
    "s3-database",
    "serverless",
    "mcp",
    "model-context-protocol",
    "mcp-server",
    "claude",
    "ai-tools",
    "llm"
  ],
  "type": "module",
  "sideEffects": false,
  "imports": {
    "#mcp/*": "./mcp/*",
    "#dist/*": "./dist/*",
    "#examples/*": "./examples/*",
    "#src/*": "./src/*",
    "#tests/*": "./tests/*"
  },
  "exports": {
    ".": {
      "types": "./dist/s3db.d.ts",
      "import": "./dist/s3db.es.js",
      "require": "./dist/s3db.cjs.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "bin/cli.js",
    "mcp/",
    "README.md",
    "PLUGINS.md",
    "SECURITY.md",
    "UNLICENSE"
  ],
  "scripts": {
    "build": "npm run build:core",
    "build:core": "rollup -c",
    "build:cli": "echo '✅ CLI uses ES modules directly from bin/cli.js (no build needed)'",
    "build:mcp": "echo '✅ MCP uses ES modules directly from mcp/entrypoint.js (no build needed)'",
    "build:all": "npm run build:core && npm run build:cli && npm run build:mcp",
    "build:binaries": "./scripts/scripts/build-binaries.sh",
    "dev": "rollup -c -w",
    "test": "pnpm run test:js && pnpm run test:ts",
    "test:js": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:quick": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --maxWorkers=2 --bail",
    "test:ts": "tsc --noEmit --project tests/typescript/tsconfig.json",
    "test:coverage": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --maxWorkers=1",
    "test:serial": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:plugins": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js tests/plugins/ --testTimeout=60000",
    "test:full": "pnpm run test:js && pnpm run test:ts",
    "benchmark": "node benchmark-compression.js",
    "benchmark:partitions": "node docs/benchmarks/partitions-matrix.js",
    "release:check": "./scripts/pre-release-check.sh",
    "validate:types": "pnpm run test:ts && echo 'TypeScript definitions are valid!'",
    "test:ts:runtime": "tsx tests/typescript/types-runtime-simple.ts",
    "prepublishOnly": "pnpm run build:all && pnpm run test:quick && echo '✅ Package ready for publish'",
    "test:mcp": "node mcp/entrypoint.js --help"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.906.0",
    "@modelcontextprotocol/sdk": "^1.19.1",
    "@smithy/node-http-handler": "^4.3.0",
    "@supercharge/promise-pool": "^3.2.0",
    "dotenv": "^17.2.3",
    "fastest-validator": "^1.19.1",
    "flat": "^6.0.1",
    "json-stable-stringify": "^1.3.0",
    "lodash-es": "^4.17.21",
    "nanoid": "5.1.6"
  },
  "peerDependencies": {
    "@aws-sdk/client-sqs": "^3.0.0",
    "@google-cloud/bigquery": "^7.0.0",
    "amqplib": "^0.10.8",
    "pg": "^8.0.0",
    "uuid": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-sqs": {
      "optional": true
    },
    "@google-cloud/bigquery": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "uuid": {
      "optional": true
    },
    "amqplib": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "24.7.0",
    "@xenova/transformers": "^2.17.2",
    "babel-loader": "^10.0.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "esbuild": "^0.25.10",
    "inquirer": "^12.9.6",
    "jest": "^30.2.0",
    "node-loader": "^2.1.0",
    "ora": "^9.0.0",
    "pkg": "^5.8.1",
    "rollup": "^4.52.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "rollup-plugin-shebang-bin": "^0.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "funding": [
    "https://github.com/sponsors/forattini-dev"
  ]
}
