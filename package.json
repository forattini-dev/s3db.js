{
  "name": "s3db.js",
  "version": "6.2.0",
  "description": "Use AWS S3, the world's most reliable document storage, as a database with this ORM.",
  "main": "dist/s3db.cjs.js",
  "module": "dist/s3db.es.js",
  "browser": "dist/s3db.iife.js",
  "types": "dist/s3db.d.ts",
  "unpkg": "dist/s3db.iife.min.js",
  "jsdelivr": "dist/s3db.iife.min.js",
  "author": "@stone/martech",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forattini-dev/s3db.js.git"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "keywords": [
    "s3",
    "aws",
    "database"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/s3db.es.js",
      "require": "./dist/s3db.cjs.js",
      "browser": "./dist/s3db.iife.js",
      "default": "./dist/s3db.es.js"
    }
  },
  "imports": {
    "#src/*": "./src/*",
    "#tests/*": "./tests/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean-build": "node scripts/clean-build.js",
    "verify": "node scripts/verify-build.js",
    "test-builds": "node examples/test-builds.js",
    "coverage": "coveralls < coverage/lcov.info",
    "coverage:serve": "npx http-server ./coverage/lcov-report",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
    "test:ai": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --runInBand",
    "test:full": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --detectOpenHandles",
    "test:watch": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --watch --detectOpenHandles"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.846.0",
    "@supercharge/promise-pool": "^3.2.0",
    "fastest-validator": "^1.19.1",
    "flat": "^6.0.1",
    "hash-wasm": "^4.12.0",
    "json-stable-stringify": "^1.3.0",
    "lodash-es": "^4.17.21",
    "nanoid": "5.1.5",
    "zlib": "^1.0.5"
  },
  "peerDependencies": {
    "@aws-sdk/client-sqs": "^3.0.0",
    "@google-cloud/bigquery": "^7.0.0",
    "amqplib": "^0.10.8",
    "pg": "^8.0.0",
    "uuid": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "@aws-sdk/client-sqs": {
      "optional": true
    },
    "@google-cloud/bigquery": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "uuid": {
      "optional": true
    },
    "amqplib": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@jest/globals": "30.0.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "30.0.4",
    "cliui": "^9.0.1",
    "coveralls": "^3.1.1",
    "dotenv": "^17.2.0",
    "esbuild": "^0.25.6",
    "fakerator": "^0.3.6",
    "fast-check": "^4.2.0",
    "jest": "30.0.4",
    "multi-progress": "^4.0.0",
    "progress": "^2.0.3",
    "rollup": "^4.45.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "uuid": "^11.1.0"
  }
}
